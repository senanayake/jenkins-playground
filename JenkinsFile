@Library('jenkins-library') _



pipeline {
    agent any
    stages {
        stage("This is the best jenkins playground stage 1") {
            steps {
                echo "This is stage 1"
                helloWorld()
            }
        }
        stage("Stage 2") {
            steps {
                echo "This is stage 2"
            }
        }
        stage("Stage 3") {
            steps {
                echo "This is stage 3"
            }
        }
        stage("Stage 4") {
            steps {
                echo "This is stage 4"
            }
        }        
    }
}
